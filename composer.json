{
  "repositories": [
    {
      "type": "git",
      "url": "/home/jh/Code/codeception/codeception"
    }
  ],
  "require": {
    "laravel/framework": "4.2.*",
    "barryvdh/laravel-ide-helper": "1.*"
  },
  "require-dev": {
    "codeception/codeception": "dev-laravel4-module"
  },
  "autoload": {
    "classmap": [
      "app/commands",
      "app/controllers",
      "app/models",
      "app/database/migrations",
      "app/database/seeds",
      "app/tests/TestCase.php"
    ]
  },
  "scripts": {
    "pre-update-cmd": [
      "php artisan clear-compiled"
    ],
    "post-install-cmd": [
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan optimize"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
